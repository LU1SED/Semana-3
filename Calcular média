function calcAverage(arr) {
  let sum = 0;
  for (let i = 0; i < arr.length; i++) {
    sum += arr[i];
  }
  return sum / arr.length;
}

let arr = [2, 4, 6, 8];
console.log(calcAverage(arr)); // 5
